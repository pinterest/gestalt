    "@types/yargs": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chalk": "^2.4.2",
    "csv-stringify": "^6.0.4",
    "dedent": "^0.7.0",
    "esbuild": "0.14.28",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.162.0",
    "fs-extra": "^8.1.0",
    "ignore": "^5.1.9",
    "jest": "^26.0.0",
    "jest-junit": "^12.0.0",
